{
  "kind": "collectionType",
  "collectionName": "cars",
  "info": {
    "singularName": "car",
    "pluralName": "cars",
    "displayName": "Car",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "namecar": {
      "type": "text",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "remaining": {
      "type": "biginteger"
    },
    "bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking.booking",
      "mappedBy": "car"
    },
    "price": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "imgcar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
